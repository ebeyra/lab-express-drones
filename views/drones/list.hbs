<h2>Available drones</h2>
{{#each allDrones}}
  <h4>{{name}}</h4>
  <ul>
    <li>Propellers: {{propellers}}</li>
    <li>Max speed: {{maxSpeed}}mph</li>
  </ul>
  <button type="submit"><a href="/drones/{{_id}}/edit">Edit</a></button>
  <form method="POST" action="/drones/{{_id}}/delete">
    <button type="submit">Delete</button>
  </form>
{{/each}}